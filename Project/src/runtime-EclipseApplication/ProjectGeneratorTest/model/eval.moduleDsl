module Evaluation {
    key "EVAL";
    cdiEnabled true;
    messageBundles {
        OrderState {
            values {
                {
                    key CONFIRMED;
                    values {
                        {
                            locale de_DE;
                            value 'Confirmed';
                        }
                        {
                            locale en_US;
                            value 'Confirmed';
                        }
                    };
                }
                {
                    key CANCLED;
                    values {
                        {
                            locale en_US;
                            value 'Cancled';
                        }
                    };
                }
            };
        }
        ErrorMessage {
            values {
                {
                    key UNKNOWN;
                    values {
                        {
                            locale en_US;
                            value 'E00002: Unhandled error detected';
                        }
                    };
                }
            };
        }
    };
    observers {
        Obs {
            type com.clevercure.event.LoginEvent;
            during InProgress;
            notify Always;
            delegate com.clevercure.evaluation.observer.LoginEventObserver;
        }
    };
    jpaConfig {
        localizedEnums {
            OrderState
        };
    };
    serviceConfig {
        messageBundles {
            OrderState ErrorMessage
        };
    };

}
